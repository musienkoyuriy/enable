{
  "name": "enabler",
  "version": "1.4.6",
  "description": "Accessibility analyzer for your frontend",
  "main": "index.ts",
  "scripts": {
    "start": "npm run-s build",
    "build": "ts-node index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint **/*.js --fix",
    "cleaninstall": "rimraf ./node_modules && npm i",
    "demo:html": "ts-node index.ts -p fixtures",
    "demo:ng": "ts-node index.ts -p fixtures --ng",
    "demo:vue": "ts-node index.ts -p fixtures --vue"
  },
  "bin": {
    "enabler": "index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/musienkoyuriy/enabler.git"
  },
  "keywords": [
    "wai-aria",
    "wcag",
    "screen-reader",
    "enabler",
    "ngx-a11y",
    "vue-a11y",
    "accessibility"
  ],
  "author": "Yuriy Musienko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/musienkoyuriy/enabler/issues"
  },
  "homepage": "https://github.com/musienkoyuriy/enabler#readme",
  "dependencies": {
    "chalk": "2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "commander": "2.15.1",
    "glob": "^7.1.2",
    "ts-node": "6.1.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-prettier": "2.6.0",
    "prettier": "1.13.4",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "2.6.2"
  },
  "eslintIgnore": [
    "fixtures/**"
  ]
}
